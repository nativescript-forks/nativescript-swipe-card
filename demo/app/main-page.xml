<Page xmlns="http://schemas.nativescript.org/tns.xsd" 
      xmlns:customControls="nativescript-swipe-card"
      loaded="pageLoaded" class="page">
    <StackLayout>
        <customControls:SwipeCard id="swipe" items="{{ stackItems }}"/>
              
          <StackLayout>
                  <Label text="{{ message }}" class="t-20 text-center c-black" textWrap="true"/>
                  <Label text="{{ version }}" class="t-20 text-center c-black" textWrap="true"/>
          </StackLayout>
  </StackLayout>
</Page>